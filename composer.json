{
    "name": "artisanry/omnipay",
    "description": "An Omnipay Bridge for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Omnipay",
        "Laravel Omnipay"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0",
        "omnipay/common": "^3.0.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Omnipay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Omnipay\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Omnipay\\OmnipayServiceProvider"
            ],
            "aliases": {
                "Omnipay": "Artisanry\\Omnipay\\Facades\\Omnipay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
