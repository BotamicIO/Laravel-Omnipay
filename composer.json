{
    "name": "faustbrian/laravel-omnipay",
    "description": "An Omnipay Bridge for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Omnipay", "Laravel Omnipay", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "omnipay/common": "^2.4"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Omnipay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Omnipay\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Omnipay\\OmnipayServiceProvider"
            ],
            "aliases": {
                "Omnipay": "BrianFaust\\Omnipay\\Facades\\Omnipay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
